
.icon{
		color: #303133;
		font-size: 40rpx;
		font-family: texticons;
}
@font-face {
		font-family: texticons;
		font-weight: normal;
		font-style: normal;
		src: url('https://at.alicdn.com/t/font_1827932_2ohvbntfb9b.ttf') format('truetype')
}
.flex{
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
}
.sp{
		-webkit-box-pack: justify;
		-webkit-justify-content: space-between;
		        justify-content: space-between;
}
.left{
		-webkit-box-pack: left;
		-webkit-justify-content: left;
		        justify-content: left;
}
.center{
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
page{
	background: #f3f4f6!important;
	height: 100%!important;
}
.bg-green{
	background: -webkit-linear-gradient(45deg,rgba(92,233,212,1) 0%,rgba(109,219,226,1) 100%);
	background: linear-gradient(45deg,rgba(92,233,212,1) 0%,rgba(109,219,226,1) 100%);
}
.font-main{
	font-size: 32rpx;
	color: #303133;
}

